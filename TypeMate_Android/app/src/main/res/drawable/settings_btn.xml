<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#e6e6e6">
    <item>
        <shape>
            <!-- 테두리 내부 색상 설정 -->
            <solid android:color="@android:color/white" />

            <!-- 테두리 설정: 두께, 색상 -->
            <stroke
                android:width="1dp"
                android:color="#ddd" />

            <!-- 둥근 정도 -->
            <corners android:radius="10dp"/>
        </shape>
    </item>
</ripple>